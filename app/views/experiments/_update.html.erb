<div class="page">
  <h3>Included Samples</h3>
  <table id="active_samples" data-search="true" data-pagination="true" data-page-size="5">
      <thead>
      <tr>
          <th data-field="name">Name</th>  
      </tr>
      </thead>
  </table>

  <h3>Update Samples</h3>
  <table id="update_samples" data-search="true" data-pagination="true" data-page-size="5" data-maintain-selected="true" data-click-to-select="true">
      <thead>
      <tr>
          <th data-field="selected" data-checkbox="true"></th>
          <th data-field="name">Name</th>  
      </tr>
      </thead>
  </table>
  <button class="btn btn-info" id="submit_button">Update</button>
</div>