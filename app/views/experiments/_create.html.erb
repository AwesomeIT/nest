<div>
  <h3>Create New</h3>
    <%= form_for(:experiment, :required => true, html: { multipart: true }) do |f| %>
      <table class="table table-striped table-bordered table-hover">
          <tr>
              <td>Name</td>
              <td><%=f.text_field :name, class: 'form-control' %></td>
          </tr>
      </table>
      <%= f.submit "Create", class: "btn btn-primary col-xs-2" %>
    <% end %>
</div>